{
    "printWidth": 120,
    "tabWidth": 4,
    "endOfLine": "lf",
    "semi": false,
    "singleQuote": false,
    "trailingComma": "es5",
    "importOrderParserPlugins": ["typescript", "jsx", "decorators-legacy"],
    "importOrder": [
        "^(next/(.*)$)|^(next$)",
        "",
        "^(react/(.*)$)|^(react$)",
        "",
        "<THIRD_PARTY_MODULES>",
        "",
        "<BUILT_IN_MODULES>",
        "",
        "^@components/(.*)$",
        "",
        "^@hooks/(.*)$",
        "",
        "^@contexts/(.*)$",
        "",
        "^@styles/(.*)$",
        "",
        "^@layouts/(.*)$",
        "",
        "^@constants/(.*)$",
        "",
        "^@services/(.*)$",
        "",
        "^@wrappers/(.*)$",
        "",
        "^@store/(.*)$",
        "",
        "^@helpers/(.*)$",
        "",
        "^@servcy/(.*)$",
        "",
        "^@types$",
        "",
        "^@/types/(.*)$",
        "",
        "^@/styles/(.*)$",
        "",
        "^@/components/(.*)$",
        "",
        "^@/utils/(.*)$",
        "",
        "^@/constants/(.*)$",
        "",
        "^[./]"
    ],
    "plugins": ["@ianvs/prettier-plugin-sort-imports"]
}
